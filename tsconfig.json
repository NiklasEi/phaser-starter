{
  "compilerOptions": {
    "target": "es2017",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "types": ["phaser"]
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
