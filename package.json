{
  "name": "phaser-starter",
  "version": "1.0.0",
  "description": "Starter project for the Phaser HTML5 game engine",
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server",
    "lint": "prettier --config .prettierrc --ignore-path .gitignore --check '**/*.{ts,json}' && eslint -c .eslintrc.json 'src/**/*.ts'",
    "format": "prettier --config .prettierrc --ignore-path .gitignore --write '**/*.{ts,json}' && eslint -c .eslintrc.json 'src/**/*.ts' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NiklasEi/phaser-starter.git"
  },
  "author": "Niklas Eicker <git@nikl.me>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/NiklasEi/phaser-starter/issues"
  },
  "homepage": "https://github.com/NiklasEi/phaser-starter#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "copy-webpack-plugin": "6.2.1",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsdoc": "30.5.1",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "3.1.3",
    "prettier": "2.0.5",
    "ts-loader": "8.0.4",
    "typescript": "4.0.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "phaser": "3.24.1"
  }
}
